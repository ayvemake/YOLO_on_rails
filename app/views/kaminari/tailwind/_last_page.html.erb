<% if current_page.last? %>
  <span class="relative inline-flex items-center px-4 py-2 text-sm font-semibold text-gray-400 ring-1 ring-inset ring-gray-300 cursor-not-allowed">
    <span class="sr-only">Last</span>
    <i class="fas fa-angle-double-right"></i>
  </span>
<% else %>
  <%= link_to url, class: "relative inline-flex items-center px-4 py-2 text-sm font-semibold text-gray-900 ring-1 ring-inset ring-gray-300 hover:bg-gray-50", remote: remote do %>
    <span class="sr-only">Last</span>
    <i class="fas fa-angle-double-right"></i>
  <% end %>
<% end %> 